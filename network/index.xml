&lt;div class="container-fluid">
&lt;div class="mb-3 row">
&lt;input type="text" class="form-control" id="ipInput" placeholder="1.1.1.1" />
&lt;/div>
&lt;div class="mb-3 row">
&lt;button class="btn btn-outline-primary btn-sm" id="query">Query&lt;/button>
&lt;/div>
&lt;div class="mb-3 row">
&lt;div class="col p-3">Organization&lt;/div>
&lt;div class="col p-3" id="ipOrganization">&lt;/div>
&lt;/div>
&lt;/div>
&lt;script>
$(function(){
$('#query').click(function(){
ip = $('#ipInput').val();
$.getJSON("https://api.ip.sb/geoip/"+ip,
function(d) {
$('#ipOrganization').text(d.organization);
}
);
});
});
&lt;/script></description></item></channel></rss>